<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="618.0" prefWidth="521.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.controller.ForgetpwdFormController">
   <children>
      <TextField fx:id="txtOTP" layoutX="136.0" layoutY="133.0" promptText="Enter OTP" style="-fx-background-color: white;">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <TextField fx:id="txtPassword" layoutX="45.0" layoutY="232.0" prefHeight="45.0" prefWidth="431.0" promptText="Enter Your New Password" style="-fx-background-color: white;" />
      <Text layoutX="45.0" layoutY="222.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password">
         <font>
            <Font name="Baloo 2 SemiBold" size="21.0" />
         </font>
      </Text>
      <TextField fx:id="txtRePassword" layoutX="45.0" layoutY="319.0" prefHeight="45.0" prefWidth="431.0" promptText="Re enter Your Password correctly" style="-fx-background-color: white;" />
      <Text layoutX="45.0" layoutY="309.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Re Password">
         <font>
            <Font name="Baloo 2 SemiBold" size="20.0" />
         </font>
      </Text>
      <Button fx:id="btnChangePassword" layoutX="175.0" layoutY="403.0" mnemonicParsing="false" onAction="#btnChangePasswordOnAction" prefHeight="45.0" prefWidth="145.0" style="-fx-border-color: black;" text="Change Password">
         <font>
            <Font name="Dyuthi Regular" size="19.0" />
         </font>
      </Button>
      <Button fx:id="btnBack" layoutX="26.0" layoutY="548.0" mnemonicParsing="false" onAction="#btnBackOnAction" prefHeight="40.0" prefWidth="83.0" style="-fx-border-color: black;" text="Back">
         <font>
            <Font name="Dyuthi Regular" size="19.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
